<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Final.SubPagesOther.FriedRice"
             Title="Fried Rice">
    <VerticalStackLayout>
        <Label 
            Text="Fried Rice"
            VerticalOptions="Center" 
            HorizontalOptions="Center" />
        <Button x:Name="Chickenfr" Text="Chicken" BackgroundColor="DarkTurquoise" TextColor="Black" FontAttributes="Bold"
        Clicked="chickenfrButton_Clicked" WidthRequest="300" VerticalOptions="End" />
        <Button x:Name="Beeffr" Text="Beef" BackgroundColor="DarkTurquoise" TextColor="Black" FontAttributes="Bold"
        Clicked="beeffrButton_Clicked" WidthRequest="300" VerticalOptions="End" />
        <Button x:Name="Shrimpfr" Text="Shrimp" BackgroundColor="DarkTurquoise" TextColor="Black" FontAttributes="Bold"
        Clicked="shrimpfrButton_Clicked" WidthRequest="300" VerticalOptions="End" />
    </VerticalStackLayout>
</ContentPage>